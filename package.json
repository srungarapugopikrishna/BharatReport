{
  "name": "janata-report",
  "version": "1.0.0",
  "description": "A comprehensive civic issue reporting and tracking platform for India",
  "main": "server/index.js",
  "engines": {
    "node": "20.x",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd server && npm install",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "start": "cd server && npm start",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:restart": "docker-compose restart",
    "docker:logs": "docker-compose logs -f",
    "docker:build": "docker-compose build",
    "setup": "npm run install-all && npm run docker:build",
    "stop": "npm run docker:down",
    "clean": "docker-compose down -v && docker system prune -f"
  },
  "keywords": [
    "civic",
    "reporting",
    "india",
    "tracking",
    "issues",
    "react",
    "nodejs",
    "postgresql",
    "docker"
  ],
  "author": "JanataReport Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/janata-report.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/janata-report/issues"
  },
  "homepage": "https://github.com/your-username/janata-report#readme",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
